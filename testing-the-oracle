+package main
+
+import (
+	"reflect"
+	"testing"
+	"time"
+)
+
+func compareVersions(t *testing.T, a, b func(int64) [][]int) {
+	const n = 10000000
+	seed := time.Now().UnixNano()
+
+	aList := a(seed)
+	bList := b(seed)
+
+	if len(aList) != n {
+		t.Errorf("version a has %d elements, expected %d", len(aList), n)
+	}
+
+	if len(bList) != n {
+		t.Errorf("version b has %d elements, expected %d", len(bList), n)
+	}
+
+	if len(aList) != len(bList) {
+		t.Errorf("expecte lists to be the same length. a had %d, b had %d", len(aList), len(bList))
+	}
+
+	if !reflect.DeepEqual(aList, bList) {
+		t.Errorf("a did not produce the same list as b")
+	}
+}
+
+func TestOracle(t *testing.T) {
+	compareVersions(t, serial, serial)
+}
